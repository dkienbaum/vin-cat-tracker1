<!DOCTYPE html>
<html>
<head>
    <title>Review Submissions</title>
</head>
<body>
    <h2>Review Submitted Vehicles</h2>
    <table border="1">
        <tr>
            <th>Date</th><th>VIN</th><th>Year</th><th>Make</th><th>Model</th>
            <th>Engine</th><th>Drivetrain</th><th>Alum</th><th>Steel</th>
            <th>Refrigerant</th><th>Converters</th>
        </tr>
        {% for entry in data %}
        <tr>
            <td>{{ entry.date }}</td>
            <td>{{ entry.vin }}</td>
            <td>{{ entry.year }}</td>
            <td>{{ entry.make }}</td>
            <td>{{ entry.model }}</td>
            <td>{{ entry.engine }}</td>
            <td>{{ entry.drivetrain }}</td>
            <td>{{ entry.aluminum_rims }}</td>
            <td>{{ entry.steel_rims }}</td>
            <td>{{ entry.refrigerant }}</td>
            <td>{{ entry.converter_qty }}</td>
        </tr>
        {% endfor %}
    </table>
</body>
</html>
